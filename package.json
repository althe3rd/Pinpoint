{
  "name": "pinpoint-accessibility-checker",
  "version": "1.5.6",
  "description": "Pinpoint Accessibility Checker - A bookmarklet for WCAG testing",
  "main": "accessibility-checker.js",
  "scripts": {
    "build": "node build.js",
    "build:extensions": "node build-extensions.js",
    "build:safari": "node -e \"require('./build-extensions.js').buildSafariOnly()\"",
    "build:safari:appstore": "node -e \"require('./build-extensions.js').buildSafariAppStore()\"",
    "watch": "node build.js --watch",
    "dev": "npm run watch",
    "serve": "python3 -m http.server 8000 || python -m http.server 8000"
  },
  "devDependencies": {
    "chokidar": "^3.5.3",
    "terser": "^5.24.0"
  },
  "keywords": [
    "accessibility",
    "a11y",
    "wcag",
    "bookmarklet",
    "browser-extension",
    "chrome-extension",
    "firefox-extension",
    "axe-core"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "gsap": "^3.13.0"
  }
}
